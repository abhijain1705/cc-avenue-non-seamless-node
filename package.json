{
  "name": "cc-aveunue-non-seamless-node",
  "version": "1.0.0",
  "description": "this is open source npm package to integrate cc avenue payment gateway with nodejs application",
  "main": "app.js",
  "scripts": {
    "start": "npm run build && node ./build/app.js",
    "start:dev": "nodemon --exec babel-node ./app.js",
